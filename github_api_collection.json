				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\": \"{{repositoryName}}\",\n    \"description\": \"This is your first repository\",\n    \"homepage\": \"https://github.com\",\n    \"private\": false,\n    \"has_issues\": true,\n    \"has_projects\": true,\n    \"has_wiki\": true\n}"
